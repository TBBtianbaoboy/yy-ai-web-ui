    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.16",
    "ahooks": "^3.1.1",
    "antd": "^4.17.3",
    "axios": "^0.24.0",
    "echarts": "^5.2.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-markdown": "^8.0.5",
    "react-syntax-highlighter": "^15.4.5",
    "umi": "^3.2.16",
    "use-react-router-breadcrumbs": "^3.0.1",
